      pipeline {
        agent {label 'sonarqube'}
        stages {
          stage("build & SonarQube analysis") {
            agent any
            steps {
              withSonarQubeEnv('SonarScanner') {
                    dir("/root/workspace/Sonarqube-pipeline/JenkinsWar") { 
                sh '''
                      mvn clean package sonar:sonar
                   '''
                }
              }
            }
          }
         }
       }

